/*画廊*/
.gallery {
  width:100%;
  margin: 0 auto;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
/*每张图片所占容器*/
.item {
  position: relative;
  margin:2rpx 2rpx;
  width: 212rpx;
  height: 212rpx;
}
.item image{
  width: 100%;
  height: 100%;
}
/*add按钮*/
.item .addIcon{
  position:relative;
  width:200rpx;
  height:200rpx;
  text-align:center;
  line-height:200rpx;
  font-size:80rpx;
  border: 5px dashed #f5f5f5;
  color: #555;
}
/*删除按钮*/
.delete {
  position:absolute;
  right:0;
  top:0;
  background:#63232;
  opacity:1;
  height: 36rpx;
  font-size:22rpx;
  font-weight:700;
  padding:0 8rpx 0 10rpx;
}
