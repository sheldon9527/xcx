.swiper-container .swiper_box {
    width: 750rpx;
    height: 250rpx;
}

.swiper-container .slide-image {
    width: 750rpx;
    height: 250rpx;
}

.swiper-container .dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    justify-content: center;
    top: 180rpx;
    height: 10rpx;
}

.swiper-container .dots .dot {
    margin: 0 5rpx;
    width: 12rpx;
    height: 8rpx;
    background: #fff;
    border-radius: 20rpx;
    transition: all 0.6s;
    opacity: 0.5;
}

.swiper-container .dots .dot.active {
    width: 22rpx;
    opacity: 1;
}

